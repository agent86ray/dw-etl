{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "dw-etl"
		},
		"bssugdw_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'bssugdw'"
		},
		"bssugstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'bssugstorage'"
		},
		"CustomerBlob_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "customer.csv"
		},
		"CustomerBlob_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "staging"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"bssugdw_connectionString": {
						"value": "[parameters('bssugdw_connectionString')]"
					},
					"bssugstorage_connectionString": {
						"value": "[parameters('bssugstorage_connectionString')]"
					},
					"CustomerBlob_properties_typeProperties_fileName": {
						"value": "[parameters('CustomerBlob_properties_typeProperties_fileName')]"
					},
					"CustomerBlob_properties_typeProperties_folderPath": {
						"value": "[parameters('CustomerBlob_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}